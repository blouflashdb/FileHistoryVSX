{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/extension/dist/**/*.{js,cjs}",
        "${workspaceFolder}/packages/extension/dist/**/*.js.map",
        "${workspaceFolder}/packages/extension/dist/**/*.cjs.map"
      ],
      "preLaunchTask": "npm: dev",
      "sourceMaps": true
    }
  ]
}
