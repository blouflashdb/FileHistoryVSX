{
  "name": "@file-history-vsx/extension",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "VS Code extension for File History VSX",
  "exports": {
    ".": "./dist/index.cjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch --sourcemap",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@file-history-vsx/types": "^0.0.1",
    "ai": "^4.3.19",
    "dotenv": "^17.2.1",
    "reactive-vscode": "^0.3.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.1.0",
    "@types/vscode": "^1.102.0",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3"
  }
}
