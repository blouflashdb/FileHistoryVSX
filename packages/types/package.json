{
  "name": "@file-history-vsx/types",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript types for File History VSX",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
