{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": false,
      "persistent": true
    },
    "lint": { "cache": false },
    "typecheck": {
      "dependsOn": ["build"],
      "cache": false
    }
  }
}
